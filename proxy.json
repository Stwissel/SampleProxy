{
    "port" : 8091,
    "useSSL" : false,
    "useProxy" : false,
    "proxyHost" : "localhost",
    "proxyPort" : 8081,
    "targetHost" : "www.spiegel.de",
    "targetPort" : 80,
    "filters" : [
        { "mimeType" : "text/html",
          "path" : "/",
          "regex" : false,
          "class" : "net.wissel.vertx.proxy.filters.HtmlFilter",
          "subfilters" : ["net.wissel.vertx.proxy.filters.html.DropLinks"]
        },
        { "mimeType" : "text/plain",
          "path" : "*",
          "regex" : false,
          "class" : "net.wissel.vertx.proxy.filters.TextFilter",
          "subfilters" : []
         }
        ],
      "deactivatedFilters" :  [
         { "mimeType" : "application/json",
           "path" : "*",
           "regex" : false,
           "class" : "net.wissel.vertx.proxy.filters.JsonFilter",
          "subfilters" : []
         }
     ]         
}